<Page x:Class="MI_HOPAC.Views.NotasInfoBoton"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MI_HOPAC.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="NotasInfoBoton">

    <Grid>
        
        <Grid.RowDefinitions>
        <RowDefinition Height="*"></RowDefinition>
        <RowDefinition Height="50"></RowDefinition>
        </Grid.RowDefinitions>

        <Frame x:Name="Frame" Grid.Row="0" NavigationUIVisibility="Hidden" ></Frame>

        <Button Grid.Row="1"  Width="100" HorizontalAlignment="Center" Click="Button_Click" Background="BurlyWood" BorderBrush="Black" >Agregar</Button>

    </Grid>
    
</Page>
