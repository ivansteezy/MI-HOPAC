<Page x:Class="MI_HOPAC.Views.MainExpedientes"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:MI_HOPAC.Views"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Page1">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>

        <Frame x:Name="expedientes_Frame" Grid.Row="0" NavigationUIVisibility="Hidden"></Frame>

        <StackPanel VerticalAlignment="Bottom" Orientation="Horizontal" HorizontalAlignment="Right" Grid.Row="1">
            <Button Click="Button_Click" x:Name="BotonNuevo" VerticalAlignment="Bottom" Margin="10">Nuevo Expediente</Button>
        </StackPanel>
    </Grid>
</Page>
